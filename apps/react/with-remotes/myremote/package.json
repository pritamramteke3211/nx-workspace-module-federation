{
  "name": "myremote",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "myremote",
    "targets": {
      "serve": {
        "options": {
          "port": 4202
        },
        "dependsOn": [
          "typecheck",
          "shell:serve"
        ]
      },
      "build": {
        "dependsOn": [
          "^build",
          "typecheck"
        ]
      }
    }
  },
  "exports": {
    "./Module": {
      "@my-workspace/source": "./src/remote-entry.ts",
      "types": "./src/remote-entry.ts",
      "import": "./src/remote-entry.ts",
      "default": "./src/remote-entry.ts"
    }
  },
  "types": "./src/remote-entry.ts"
}
